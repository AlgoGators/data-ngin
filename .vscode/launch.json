{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Python Debugger: Module",
        "type": "debugpy",
        "request": "launch",
        "module": "data",
        "cwd": "${workspaceFolder}",
        "console": "integratedTerminal"
      },
      {
        "name": "Python: Current File",
        "type": "debugpy",
        "request": "launch",
        "program": "${file}",
        "cwd": "${workspaceFolder}",
        "console": "integratedTerminal",
      }
    ]
  }
  